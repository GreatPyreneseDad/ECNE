{
  "name": "ecne-data-river",
  "version": "0.1.0",
  "description": "Enhanced Coherence Network Engine - Data River Agent for coherence-based data filtering",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx scripts/start-dev.ts",
    "dev:unsafe": "tsx scripts/start-dev.ts --unsafe",
    "dev:watch": "tsx watch src/index-safe.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "tsx scripts/run-tests.ts all",
    "test:basic": "tsx scripts/run-tests.ts basic",
    "test:stress": "tsx scripts/run-tests.ts stress",
    "test:error": "tsx scripts/run-tests.ts error",
    "test:filter": "tsx scripts/run-tests.ts filter",
    "example": "tsx examples/quick-start.ts",
    "lint": "eslint src --ext .ts,.tsx",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate"
  },
  "keywords": [
    "coherence",
    "data-stream",
    "api-aggregation",
    "gct",
    "real-time"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "axios": "^1.6.8",
    "bull": "^4.12.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.3",
    "p-queue": "^8.0.1",
    "socket.io": "^4.7.4",
    "winston": "^3.12.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.11.30",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prisma": "^5.11.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  }
}